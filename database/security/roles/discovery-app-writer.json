{
  "role-name": "discovery-app-writer",
  "description": "role for contributing to samplestack corpus",
  "role": [
      "rest-writer", "discovery-app-guest", "rest-admin"
  ],
  "permission": [
    {
      "role-name": "rest-reader",
      "capability": "read"
    },
    {
      "role-name": "rest-writer",
      "capability": "read"
    },
    {
      "role-name": "rest-writer",
      "capability": "update"
    },
    {
      "role-name": "rest-admin",
      "capability": "read"
    },
    {
      "role-name": "rest-admin",
      "capability": "update"
    }
  ],
  "privilege": [
    {
      "privilege-name": "xdmp:filesystem-directory",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-filesystem-directory",
      "kind": "execute"
    },
    {
      "privilege-name": "xdmp:filesystem-file",
      "action": "http://marklogic.com/xdmp/privileges/xdmp-filesystem-file",
      "kind": "execute"
    },
    {
      "privilege-name": "unprotected-collections",
      "action": "http://marklogic.com/xdmp/privileges/unprotected-collections",
      "kind": "execute"
    }
  ]
}
